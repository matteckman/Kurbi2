<%= simple_form_for(@daily_record) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :wellness_score %>
    <%= f.input :note %>
    <%= f.input :day_id %>
    <%= f.input :patient_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
