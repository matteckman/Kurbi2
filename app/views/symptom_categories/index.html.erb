<h1>Listing symptom_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @symptom_categories.each do |symptom_category| %>
  <tr>
    <td><%= symptom_category.name %></td>
    <td><%= symptom_category.description %></td>
    <td><%= link_to 'Show', symptom_category %></td>
    <td><%= link_to 'Edit', edit_symptom_category_path(symptom_category) %></td>
    <td><%= link_to 'Destroy', symptom_category, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Symptom category', new_symptom_category_path %>
